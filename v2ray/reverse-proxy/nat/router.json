            {
                "tag": "pro",
                "domain": "v2.reverse.proxy"
            }
        ]
    },
    "inbounds": [],
    "outbounds": [
        {
            "tag": "pro-tunnel",
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "jpword.home-yyou.top",
                        "port": 443,
                        "users": [
                            {
                                "id": "897244b5-0170-44a3-9f76-672fa6734c9b",
