{
    "log": {
        "access": "reverse_router.log",
        "error": "reverse_router.log",
        "loglevel": "warning"
    },
    "reverse": {
        "portals": [
            {
                "tag": "portal_router_asus",
                "domain": "router_asus.reverse.proxy"
            },
            {
                "tag": "portal_server_ubuntu",
                "domain": "server_ubuntu.reverse.proxy"
            }
        ]
    },
    "inbounds": [
        {
            // nat 连这里
            "tag": "nats",
            "listen": "127.0.0.1",
            "port": 8080,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "897244b5-0170-44a3-9f76-672fa6734c9b"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/nat"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "metadataOnly": false
            }
        },
        {
            // 需要连接 asus的客户端连这个
            "tag": "client_router_asus",
            "listen": "127.0.0.1",
            "port": 9090,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "897244b5-0170-44a3-9f76-672fa6734c9b"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/client/router"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "metadataOnly": false
            }
        },
        {
            // 需要连接 ubuntu的客户端连这个
            "tag": "client_server_ubuntu",
            "listen": "127.0.0.1", 
            "port": 9091,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "897244b5-0170-44a3-9f76-672fa6734c9b"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/client/server"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "metadataOnly": false
            }
        }
    ],
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "nats"
                ],
                "domain": [
                    "full:router_asus.reverse.proxy"
                ],
                "outboundTag": "portal_router_asus"
            },
            {
                "type": "field",
                "inboundTag": [
                    "nats"
                ],
                "domain": [
                    "full:server_ubuntu.reverse.proxy"
                ],
                "outboundTag": "portal_server_ubuntu"
            },
            {
                "type": "field",
                "inboundTag": [
                    "client_router_asus"
                ],
                "outboundTag": "portal_router_asus"
            },
            {
                "type": "field",
                "inboundTag": [
                    "client_server_ubuntu"
                ],
                "outboundTag": "portal_server_ubuntu"
            }
        ]
    }
}